{
	"name": "next-w-bun",
	"version": "0.0.135",
	"scripts": {
		"build": "next build",
		"start": "next start",
		"clean": "rm -rf dist && echo 'Done.'",
		"postinstall": "husky install",
		"check-types": "tsc --pretty --noEmit",
		"check-format": "prettier --check .",
		"check-eslint": "next lint",
		"check-lint": "eslint --ext ts --ext tsx --ext js --fix",
		"format": "prettier --write .",
		"prepare": "husky install"
	},
	"dependencies": {
		"next": "latest",
		"react": "^18",
		"react-dom": "^18",
		"react-is": "^18"
	},
	"devDependencies": {
		"@types/node": "18.15.11",
		"@types/react": "^18",
		"@typescript-eslint/eslint-plugin": "^5.57.1",
		"bun-framework-next": "^12.2",
		"eslint": "^8.37.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-config-standard-with-typescript": "^34.0.1",
		"eslint-plugin-import": "^2.27.5",
		"eslint-plugin-n": "^15.7.0",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-promise": "^6.1.1",
		"eslint-plugin-react": "latest",
		"husky": "^8.0.3",
		"lint-staged": "^13.2.0",
		"pinst": "^3.0.0",
		"prettier": "^2.8.7",
		"react-refresh": "0.10.0",
		"typescript": "^5.0.3"
	},
	"module": "index.js",
	"lint-staged": {
		"*.js": "eslint --cache --fix"
	},
	"husky": {
		"hooks": {
			"pre-commit": " --pretty --noEmit && prettier --check . && next lint && eslint --ext ts --ext tsx --ext js --fix prettier --write . prettier --write . && git add -A ."
		}
	}
}
